@model MinerTrabajoFInal.Models.Resultado

@{
    ViewData["Title"] = "Editar";
}

<h1>Editar</h1>
<hr />
<div class="container">
    <div class="col-md-4">
        
        <form  asp-action="Editar">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                               
           <div class="form-group text-left" style="">
                <label asp-for="numerorecpcion" class="control-label"></label>
                <input asp-for="numerorecpcion" class="form-control"> 
                <span asp-validation-for="numerorecpcion" class="text-danger"></span>
            </div><br>      
            <div class="form-group text-left" style="">
                <label asp-for="tipo_analisis" class="control-label"></label>
                <input asp-for="tipo_analisis" class="form-control"> 
                <span asp-validation-for="tipo_analisis" class="text-danger"></span>
            </div><br>   
              <div class="form-group text-left" style="">
                <label asp-for="Envoltura" class="control-label"></label>
                <input asp-for="Envoltura" class="form-control"> 
                <span asp-validation-for="Envoltura" class="text-danger"></span>
            </div><br>               
             <div class="form-group text-left" style="">
                <label asp-for="valor" class="control-label"></label>
                <input asp-for="valor" class="form-control"> 
                <span asp-validation-for="valor" class="text-danger"></span>
            </div><br>               
            <div class="form-group">
                 <button class="btn btn-success">Actualizar</button>
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index"></a>
</div>