<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MinerTrabajoFInal</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        <nav class="sticky-topnavbar bd-navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3 ">
            <div class="bg-danger sticky-top" style="width:100%; font-family:Metrophobic;">
                
                
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <partial name="_LoginPartial" />
                    <ul class="navbar-nav flex-grow-1">
                        <li>
                          <li class="nav-item">
                            <a class="nav-link text-white"  asp-area="" asp-controller="Home" asp-action="Index"><img src="https://e7.pngegg.com/pngimages/953/836/png-clipart-crystal-mineral-solid-product-design-industry-crystal-icon-angle-hand.png"  class="img-thumbnail" height="50px" width="50px"></a>
                        </li>  
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-white" style="font-size:20px;" asp-area="" asp-controller="Home" asp-action="Index">MineraLab</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-white" style="font-size:20px;" asp-area="" asp-controller="Recepcion" asp-action="Index">Recepcion</a>
                        </li>  
                        <li class="nav-item">
                            <a class="nav-link text-white" style="font-size:20px;" asp-area="" asp-controller="Resultado" asp-action="Index">Resultado</a>
                        </li> 
                        <partial name="_MenuPartial" />
                    </ul>
                </div>
            </div>
        </nav>
          <div class="row">
                <div class="col-sm-12">
                    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                        <div class="carousel-inner">
                            <div class="carousel-item active">
                                <img src="https://www.aeuroweb.com/wp-content/uploads/2017/03/twitter.jpg" class="d-block w-100" alt="bootstrap" 
                                width="1200px" height="100px">
                            </div>
                            <div class="carousel-item">
                                <img src="https://www.aeuroweb.com/wp-content/uploads/2018/04/medidas-instagram.jpg" class="d-block w-100" alt="..." 
                                width="1200px" height="100px">
                            </div>
                            <div class="carousel-item">
                                <a href="https://developer.mozilla.org/es/docs/Web/JavaScript" target="_blanck">
                                    <img src="https://www.aeuroweb.com/wp-content/uploads/2018/04/medidas-facebook.jpg" class="d-block w-100" alt="..." 
                                        width="1200px" height="100px">
                                </a>
                                
                            </div>
                        </div>
                        <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                            <span class="sr-only">Previous</span>
                        </a>
                        <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                            <span class="sr-only">Next</span>
                        </a>
                    </div>
                </div>
            </div>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - MinerTrabajoFInal - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
